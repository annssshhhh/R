install.packages("ggplot2")
library(dplyr)
library(ggplot2)
View(diamonds)
?diamonds
ggplot()
ggplot(data = diamonds, aes(x=price)) + geom_histogram(fill="violet", col="black")
ggplot(data = diamonds, aes(x=cut)) + geom_bar(fill="pink", col="red")
ggplot(data = diamonds, aes(x=cut, fill=cut)) + geom_bar()
ggplot(data = diamonds, aes(x=cut, fill=color)) + geom_bar(col="grey")
